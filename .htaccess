# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.php [L]
</IfModule>

[code autolinks="false"]
RewriteEngine On
RewriteBase /
RewriteCond %{HTTP_HOST} ^www.mbird.com
RewriteRule ^publications/(.*)$ https://store.mbird.com/$1 [L,R=301]
[/code]
# END WordPress
